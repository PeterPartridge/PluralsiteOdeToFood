@model HomeIndexViewModel;
@{
    ViewBag.Title = "Home Page";
}
<div>
    <h1> Here is a list of restaurants</h1>

    <div>
        @foreach (var item in Model.Restaurants)
        {
            @Html.Partial("_Summery", item);

        }


        <div><a asp-action="Create" asp-controller="Home">Add a restaurant</a></div>
    </div>
</div>
